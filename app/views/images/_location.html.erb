
  <% @warmest.each do |image| %> 
  <div class="square">
    <div class="image">
      <%= image_tag image[1].photo.url(:small) %>
    </div>
    <div class="distance">
      <%= link_to(image_tag(image[2], :class => "nav"), "http://maps.google.com/maps?z=12&t=m&q=loc:#{image[4][0]}+#{image[4][1]}") %>
      <div class="miles_away">
        <p><%= image[3].round(2) %> miles away</p> 
      </div>
      <span class="timestamp"><%= image[0] %></span>
      <p>
        <span class="tag">#king</span>
        <span class="tag">#all-city</span>
        <span class="tag">#burner</span>
      </p>
    </div>
  </div>
  <br>
  <% end %>
